<?xml version="1.0" encoding="utf-8" ?>
<dataOperations xmlns="http://oversea.newegg.com/DataOperation"
				xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <dataCommand name="AddUserWatchInfo" database="EasyBuyCompany" commandType="Text">
    <commandText>
      <![CDATA[                             
INSERT INTO [dbo].[UserWatch]
   ([UserID]
   ,[WatchID]
   ,[WatchType]
   ,[InDate]
   ,[InUser]
   ,[LastEditDate]
   ,[lastEditUser])
VALUES
  (
  @UserID,
  @WatchID,
  @WatchType,
  GETDATE(),
  @InUser,
  GETDATE(),
  @InUser
  )
          ]]>
    </commandText>
  </dataCommand>

  <dataCommand name="GetUserWatchInfo" database="EasyBuyCompany" commandType="Text">
    <commandText>
      <![CDATA[    
SELECT @TotalCount=COUNT(1)  
  FROM [dbo].[UserWatch] WITH(NOLOCK)
#StrWhere#

SELECT [UserID],
       [WatchID],
       [WatchType],
       [InDate],
       [InUser],
       [LastEditDate],
       [LastEditUser]
FROM [dbo].[UserWatch] WITH(NOLOCK)
#StrWhere#
ORDER BY #SortColumnName#
          ]]>
    </commandText>
  </dataCommand>
  
<dataCommand name="RemoveUserWatchInfo" database="EasyBuyCompany" commandType="Text">
    <commandText>
      <![CDATA[    
DELETE
FROM [dbo].[UserAddress]
#StrWhere#
          ]]>
    </commandText>
  </dataCommand>
  
</dataOperations>
